    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "npm-run-all": "^2.1.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "concurrently": "^9.1.0",
    "react": "^18.3.1",
